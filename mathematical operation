def math_operations(x, n, choice):
    if choice == 1:
        return x ** n
    elif choice == 2:
        return x + n
    elif choice == 3:
        return x - n
    elif choice == 4:
        return x * n
    elif choice == 5:
        return x / n

x = float(input("X = "))
n = float(input("N = "))
choice = int(input("Choice: "))
result = math_operations(x, n, choice)
print(f"Result: {result}")
